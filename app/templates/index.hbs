{{layer-picker
  onLayerUpdate=(action "onLayerUpdate")
  layersChosen=(readonly model.layersChosen)
  layersAvailable=(readonly model.layersAvailable)}}

{{#if layers}}
{{geoserver-leaflet
  layers=model.layersChosen
  lat=(readonly model.lat)
  lng=(readonly model.lng)
  zoom=(readonly model.zoom)
  mapViewChanged=(action "mapViewChanged")}}
{{/if}}

{{outlet}}
